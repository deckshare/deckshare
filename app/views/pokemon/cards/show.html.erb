<div class="w-1/3 pr-8">
  <%= image_tag @card.images.large %>
</div>
<div class="w-2/3">
  <h1 class="font-bold text-4xl"><%= @card.name %></h1>
  <% @card.types&.each do |t| %>
    <i class="energy <%= t.downcase %>"></i>
  <% end %>
  <p>Find me in app/views/pokemon/cards/show.html.erb</p>
</div>
